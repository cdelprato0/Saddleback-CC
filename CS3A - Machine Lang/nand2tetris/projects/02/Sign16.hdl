/**
* 
	Sign16:
* 
	out = in[15]

*/



CHIP Sign16 {
    
	IN in[16];
    
	OUT out;
    
    
  PARTS:
  
	Not( in = in[15], out = output );
	Not( in = output, out = out );      

}